{
	"compilerOptions": {
		"module": "commonjs",
		"target": "ES2020",
		"noImplicitAny": false,
		"lib": [
			"ES2020"
		],
		"esModuleInterop": true,
		"strictPropertyInitialization": false,
		"strictNullChecks": false,
		"sourceMap": true,
		"rootDir": "src",
		"strict": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
